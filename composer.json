{
  "name": "fschmid-dev/contao-leaflet-map-bundle",
  "type": "contao-bundle",
  "description": "Contao Bundle to include a new frontend module for leaflet map integration",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Florian Schmid",
      "email": "info@fschmid.dev",
      "homepage": "https://fschmid.dev"
    }
  ],
  "require": {
    "php": ">= 7.4 || ^8",
    "ext-curl": "*",
    "contao/conflicts": "@dev",
    "contao/core-bundle": "^4.13",
    "mvo/contao-group-widget": "^1.4",
    "symfony/event-dispatcher": "^5.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.11",
    "roave/security-advisories": "dev-latest"
  },
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "contao-components/installer": true,
      "contao/manager-plugin": true
    }
  },
  "extra": {
    "contao-manager-plugin": "FSchmidDev\\LeafletMapBundle\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "FSchmidDev\\LeafletMapBundle\\": "src/"
    }
  }
}
